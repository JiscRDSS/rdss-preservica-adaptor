[general]
state_file = /var/awslogs/state/agent-state

[/var/log/preservicaservice/debug.log]
file = /var/log/preservicaservice/debug.log
log_group_name = preservicaservice-${terraform_environment}-/var/log/preservicaservice/debug.log
log_stream_name = ${instance_type}/{instance_id}
datetime_format = %Y-%m-%dT%H:%M:%S%z

